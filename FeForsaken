	local Anim
	local id
	local walk = 18885906143
	local mouse1 = false
	local finish = false
	local i = 18885903667
	local m1 = 18885909645
	local fid = 89669118763885
	local sprinting1 = false
	local pizza = false
	local pizzaid = 18885915433
	local CN = false
	local CNid = 18885919947
	local run = 96571077893813
	local uis = game:GetService("UserInputService")
	local walking = false
	local hwalking = 132377038617766
	local hurt = false
	local hidle = 134624270247120
	local hrun = 115946474977409
	local sprinting = false
	local WO = false
	local WOsid = 18885932392
	local WOl = 18885939653
	local WOe = 18885940850
	local taunt = false
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local mouse = player:GetMouse()
	local idle = true
	local char = game.Players.LocalPlayer.Character
	local hum = char:FindFirstChild("Humanoid")
	Anim = hum:LoadAnimation(i)
	Anim:Play()
	id = i
	hum.WalkSpeed = 7.5
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.LeftShift and CN == false and pizza == false then
			sprinting1 = true
			hum.WalkSpeed = 28
		end
	end)
	uis.InputEnded:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.LeftShift and CN == false and pizza == false then
			sprinting1 = false
			hum.WalkSpeed = 7.5
		end
	end)
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.One then
			Anim:Stop()
			id = "taunt"
			Anim = hum:LoadAnimation(111309618966926)
			taunt = true
			Anim:Play()
		end
	end)
	
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.T and taunt == false and pizza == false then
			if id ~= fid then 
				hum.WalkSpeed = 0
				sprinting1 = false
				finish = true
				Anim:Stop(2)
				Anim = hum:LoadAnimation(fid)
				Anim.Looped = false
				Anim:Play()
				id = fid
				wait(4)
				finish = false
				hum.WalkSpeed = 6
	
	
			end
		end
	end)
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.Q and pizza == false and finish == false then
			sprinting1 = false
			wait(.1)    
			CN = true
			hum.WalkSpeed = 0
			wait(.5)
			CN = false
			hum.WalkSpeed = 9
		end
	end)
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.R and CN == false and finish == false  then
			sprinting1 = false
			wait(.1)    
			pizza = true
			hum.WalkSpeed = 0
			wait(2.6)
			pizza = false
			hum.WalkSpeed = 9
		end
	end)
	local function onButton1Down()
		if id ~= m1 then 
			mouse1 = true
			Anim:Stop()
			Anim = hum:LoadAnimation(m1)
			Anim.Looped = false
			Anim:Play()
			id = m1
			wait(.5)
			mouse1 = false
		end
	end
	
	mouse.Button1Down:Connect(onButton1Down)
	while wait() do
		if idle == true and taunt == false and hurt == false and CN == false and pizza == false and mouse1 == false and finish == false then
			if id ~= i then 
				Anim:Stop(2)
				Anim = hum:LoadAnimation(i)
				Anim.Looped = true
				Anim:Play()
				id = i
			end
		end
		if walking == true and taunt == false and hurt == false and sprinting1 == false and CN == false and pizza == false and finish == false and mouse1 == false then
			if id ~= walk then
				Anim:Stop(2)
				Anim = hum:LoadAnimation(walk)
				Anim.Looped = true
				Anim:Play() 
				id = walk
			end
		end
		if sprinting == true and taunt == false and hurt == false and mouse1 == false then
			if id ~= run then
				Anim:Stop(2)
				Anim = hum:LoadAnimation(run)
				Anim.Looped = true
				Anim:Play() 
				id = run
			end
		end
		if hum.MoveDirection ~= Vector3.new(0, 0, 0) and sprinting1 == false then
			walking = true
			sprinting = false
			idle = false
			taunt = false
		end
		if hum.MoveDirection ~= Vector3.new(0, 0, 0) and sprinting1 == true and mouse1 == false and finish == false  then
			walking = false
			sprinting = true
			idle = false
			taunt = false
		end
		if hum.MoveDirection == Vector3.new(0,0,0) then
			idle = true
			walking = false
			sprinting = false
		end
		if idle == true and taunt == false and mouse1 == false and hurt == true and CN == false and pizza == false and finish == false  then
			if id ~= hidle then 
				Anim:Stop(2)
				Anim = hum:LoadAnimation(hidle)
				Anim.Looped = true
				Anim:Play()
				id = hidle
			end
		end
		if walking == true and taunt == false and mouse1 == false and hurt == true and sprinting1 == false and attack == false and pizza == false and finish == false  then
			if id ~= hwalking then
				Anim:Stop(2)
				Anim = hum:LoadAnimation(hwalking)
				Anim.Looped = true
				Anim:Play() 
				id = hwalking
			end
		end
		if sprinting == true and taunt == false and hurt == true and mouse1 == false then
			if id ~= hrun then
				Anim:Stop(2)
				Anim = hum:LoadAnimation(hrun)
				Anim.Looped = true
				Anim:Play() 
				id = hrun
			end
		end
		if pizza == true and taunt == false then
			if id ~= pizzaid then 
				Anim:Stop()
				Anim = hum:LoadAnimation(pizzaid)
				Anim.Looped = false
				Anim:Play()
				id = pizzaid
			end
		end
		if CN == true and taunt == false then
			if id ~= CNid then 
				Anim:Stop()
				Anim = hum:LoadAnimation(CNid)
				Anim.Looped = false
				Anim:Play()
				id = CNid
			end
		end
	
		end
		end)
end
coroutine.wrap(OJEO_script)()
local function IUYJ_script() -- Respawn.LocalScript 
	local script = Instance.new('LocalScript', Respawn)

	script.Parent.MouseButton1Down:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end)
end
coroutine.wrap(IUYJ_script)()
local function QIYZVI_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Down:Connect(function()
		game.Players.LocalPlayer.PlayerGui.Forsaken:Destroy()
	end)
end
coroutine.wrap(QIYZVI_script)()
local function LSNSITK_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	--This script has been modified by MetaliumWorks (@jgfuhjkkl_2) [mw/ÊÉ¯]
	
	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    dragSpeed = 0.25
	    dragInput = nil
	    dragStart = nil
	    dragPos = nil
	    function updateInput(input)
	        Delta = input.Position - dragStart
	        Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.20, Enum.EasingStyle.Back), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(LSNSITK_script)()
local function SEUUIJ_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	script.Parent.MouseButton1Down:Connect(function()
		game.Players.LocalPlayer.PlayerGui.Forsaken.Close.Visible = true
		script.Parent.Parent.Parent.Frame.Visible = false
	end)
end
coroutine.wrap(SEUUIJ_script)()
local function QWUPX_script() -- Invite.LocalScript 
	local script = Instance.new('LocalScript', Invite)

	while true do
		wait(0)
		script.Parent.Text = "https://dsc.gg/astrolua"
	end
end
coroutine.wrap(QWUPX_script)()
local function ZXUF_script() -- Close_2.LocalScript 
	local script = Instance.new('LocalScript', Close_2)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Visible = false
		game.Players.LocalPlayer.PlayerGui.Forsaken.Frame.Visible = true
	end)
end
coroutine.wrap(ZXUF_script)()
